{
    "version": 2,
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "frontend/$1"
        }
    ],
    "env": {
        "NEXT_PUBLIC_BACKEND_URL": "@backend-url",
        "NEXT_PUBLIC_CAMP_TESTNET_RPC": "https://rpc.camp.network/testnet",
        "NEXT_PUBLIC_CHAIN_ID": "325000"
    }
}